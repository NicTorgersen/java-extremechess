package gamelogic;
import pieces.ChessPiece;
import ui.Square;

public class Movement {
	
	
	public void move(Square movefrom, Square moveto) {
		ChessPiece piece = movefrom.getChessPiece();
		
		if(moveIsLegal(piece)){
			
		}
		
        moveto.movePiece(movefrom.getPiece());
        movefrom.removePiece();
    }
	
	private boolean moveIsLegal(ChessPiece piece){
		return true;
	}
}
